{
	"$schema": "https://json.schemastore.org/swcrc",
	"jsc": {
		"parser": {
			"syntax": "typescript",
			"tsx": true,
			"decorators": true
		},
		"target": "es2020",
		"loose": false,
		"minify": {
			"compress": false,
			"mangle": false
		}
	},
	"module": {
		"type": "es6",
		"strict": true,
		"strictMode": true,
		"lazy": false,
		"noInterop": false
	},
	"minify": false,
	"isModule": true
}
